    "@web/test-runner": "^0.18.0",
    "eslint": "^8.53.0",
    "npm-run-all": "^4.1.5",
    "qunit": "^2.20.0",
    "rimraf": "^5.0.5",
    "tslib": "^2.6.2",
    "typescript": "5.2.2"
